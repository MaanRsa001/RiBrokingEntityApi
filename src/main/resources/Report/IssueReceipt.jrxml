	</parameter>
	<queryString>
		<![CDATA[/* Formatted on 21-04-2018 05:45:52 PM (QP5 v5.115.810.9015) */
SELECT   '' COMPANY_NAME,
         B.FIRST_NAME || B.LAST_NAME AS BROKER,
         C.SHORT_NAME CURRENCY_NAME,
         R.PAID_AMT,
         TO_CHAR (TRANS_DATE, 'DD/MM/YYYY') AS TRANSDATE,
         PAYMENT_RECEIPT_NO,
         R.EXCHANGE_RATE,
         ROUND ( (R.PAID_AMT / R.EXCHANGE_RATE), 2) TOT_EXC_AMT,
         R.BROKER_ID,
         R.DIFF_AMT,
         R.TRANSCATIONTYPE,
         R.CURRENCY_ID,
         R.REVERSALTRANSNO,
         R.BANK_CHARGES,
         R.NET_AMT,
         R.CONVERTED_DIFF_AMT,
         BM.BANK_NAME,
	R.REMARKS,
         WH_TAX,
         cmp.*
  FROM            TTRN_PAYMENT_RECEIPT R,
                  PERSONAL_INFO B,CURRENCY_MASTER C,BANK_MASTER BM,
                  (SELECT   COMPANY_NAME,
         EMAIL,
         ADDRESS1,
         COUNTRY,
         AMEND_ID,
         STATUS,
         WEB_SITE,
         TELEPHONE
  FROM   company_master
 WHERE   COMPANY_CODE = 'RI01' AND STATUS = 'Y' ) cmp


  where    B.CUSTOMER_ID = R.BROKER_ID AND B.BRANCH_CODE =$P{branchCode}
AND B.CUSTOMER_TYPE = 'B' and   BM.BANK_ID = R.RECEIPT_BANK
AND BM.BRANCH_CODE = R.BRANCH_CODE
and C.CURRENCY_ID = R.CURRENCY_ID AND C.BRANCH_CODE = $P{branchCode}
and BM.BANK_ID = R.RECEIPT_BANK
            AND BM.BRANCH_CODE = R.BRANCH_CODE
            AND BM.STATUS = 'Y'
 and   R.PAYMENT_RECEIPT_NO =  $P{receiptNo} AND R.BRANCH_CODE = $P{branchCode}
 AND R.AMEND_ID =
                        (SELECT   MAX (AMEND_ID)
                           FROM   TTRN_PAYMENT_RECEIPT
                          WHERE   PAYMENT_RECEIPT_NO = R.PAYMENT_RECEIPT_NO
                                  AND BRANCH_CODE = R.BRANCH_CODE)
                  AND B.AMEND_ID =
                        (SELECT   MAX (AMEND_ID)
                           FROM   PERSONAL_INFO
                          WHERE   CUSTOMER_ID = B.CUSTOMER_ID
                                  AND BRANCH_CODE = B.BRANCH_CODE)
                                                 AND C.AMEND_ID =
                     (SELECT   MAX (AMEND_ID)
                        FROM   CURRENCY_MASTER
                       WHERE   CURRENCY_ID = C.CURRENCY_ID
                               AND BRANCH_CODE = C.BRANCH_CODE)]]>
	</queryString>
