		<parameter name="broker" class="java.lang.String"/>
		<parameter name="ceding" class="java.lang.String"/>
		<parameter name="payrectype" class="java.lang.String"/>
		<parameter name="transactionType" class="java.lang.String"/>
		<queryString>
			<![CDATA[Select Payment_Receipt_No,To_Char(Tpr.Trans_Date,'DD/MM/YYYY') Trans_Date,To_Char(Tpr.Amendment_Date,'DD/MM/YYYY') Amendment_Date ,
Decode(Trans_Type,'PT','PAYMENT','RECEIPT') Trans_Type,(Select First_Name From Personal_Info Pi Where Pi.Customer_Id=Tpr.Broker_Id And Tpr.Branch_Code=Pi.Branch_Code)Broker_Name,(Select First_Name From Personal_Info Pi Where Pi.Customer_Id=Tpr.Ceding_Id And Tpr.Branch_Code=Pi.Branch_Code) Ceding_Id,(Select Bank_Name From Bank_Master Bm Where Bm.Bank_Id=Receipt_Bank And Bm.Branch_Code=Tpr.Branch_Code) Receipt_Bank,(Select Currency_Name From Currency_Master Cm Where Cm.Currency_Id=Tpr.Currency_Id And Cm.Branch_Code=Tpr.Branch_Code And Amend_Id=(Select Max(Amend_Id) From Currency_Master C Where C.Currency_Id=Cm.Currency_Id And C.Branch_Code=Cm.Branch_Code)) Currency_Id,
Tpr.Exchange_Rate,Paid_Amt Gross_Amt,Bank_Charges,'' Holdtaxrecover_Oc,Net_Amt,Round((Paid_Amt/Tpr.Exchange_Rate),2) Gross_Amt_Dc,
Round((Bank_Charges/Tpr.Exchange_Rate),2)  Bank_Charges_Dc,'' Holdtaxrecover_Ugx,Round((Net_Amt/Tpr.Exchange_Rate),2) Net_Amt_Dc,
Decode(Sign(Diff_Amt),'-1','SUBTRACT','ADD') Diff_Type,
Diff_Amt Exchangedifferenceamount,Round(((Diff_Amt/(Paid_Amt/Tpr.Exchange_Rate)) * 100),2) Exchage_Diff_Perc,Transcationtype,Reversaltransno,Tpr.Remarks
,'' sTATUS
From Ttrn_Payment_Receipt Tpr,Ttrn_Payment_Receipt_Details Tprd
Where Tpr.Payment_Receipt_No=Tprd.Receipt_Sl_No And
Tprd.Currency_Id !=0 and branch_code=$P{branchCode}
and tpr.TRANS_DATE BETWEEN TO_DATE($P{startDate},'DD/MM/YYYY')AND TO_DATE($P{endDate},'DD/MM/YYYY')
AND upper(DECODE($P{ceding},'-1','A',Ceding_Id))=upper(Decode($P{ceding},'-1','A',$P{ceding}))  AND upper(DECODE($P{broker},'-1','A',BROKER_ID))=upper(Decode($P{broker},'-1','A',$P{broker}))
AND upper(DECODE($P{payrectype},'ALL','A',TRANS_TYPE))=upper(Decode($P{payrectype},'ALL','A',$P{payrectype}))  AND upper(DECODE($P{transactionType},'ALL','A',TRANSCATIONTYPE))=upper(Decode($P{transactionType},'ALL','A',$P{transactionType}))
and tpr.amend_id=(select max(amend_id) from Ttrn_Payment_Receipt where  PAYMENT_RECEIPT_NO=tpr.PAYMENT_RECEIPT_NO and branch_code=tpr.branch_code) and tpr.amend_id=Tprd.amend_id]]>
		</queryString>
